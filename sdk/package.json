        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^3.0.3",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ng-packagr": "^1.7.0",
        "protractor": "~5.4.4",
        "rimraf": "^3.0.1",
